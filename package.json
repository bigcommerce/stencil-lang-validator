{
  "name": "stencil-lang-validator",
  "version": "0.1.0",
  "description": "Validate language keys used in templates and scripts",
  "homepage": "https://github.com/bigcommerce-labs/stencil-lang-validator",
  "repository": {
    "type": "git",
    "url": "https://github.com/bigcommerce-labs/stencil-lang-validator.git"
  },
  "bugs": {
    "url": "https://github.com/bigcommerce-labs/stencil-lang-validator/issues"
  },
  "author": "BigCommerce",
  "keywords": [
    "stencil",
    "language",
    "validation"
  ],
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "main": "lib/index.js",
  "bin": {
    "validate-lang": "bin/cli.js"
  },
  "scripts": {
    "prebuild": "rm -rf bin lib && npm test",
    "build": "NODE_ENV=production babel src --out-dir . --source-maps",
    "coverage": "nyc npm test",
    "lint": "eslint src test",
    "pretest": "npm run lint",
    "test": "JASMINE_CONFIG_PATH=.jasmine.json babel-node node_modules/.bin/jasmine"
  },
  "dependencies": {
    "babel-runtime": "^6.22.0",
    "babylon": "^6.15.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "glob": "^7.1.1",
    "lodash": "^4.17.4",
    "ora": "^1.1.0",
    "pad": "^1.0.2",
    "pkg-dir": "^1.0.0",
    "pluralize": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "eslint": "^3.14.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "jasmine": "^2.5.3",
    "nyc": "^10.1.2"
  }
}
